@model Proyecto.Usuarios

@{
    ViewBag.Title = "Delete";
}

<h2>Eliminar Usuario</h2>

<h3>Estás seguro que quieres eliminar esto?</h3>
<div>
    <h4>Usuarios</h4>
    <hr />
    <dl class="dl-horizontal">
        <dt>
            @Html.Label("Nombre")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Nombre)
        </dd>

        <dt>
            @Html.Label("Apellido")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Apellido)
        </dd>

        <dt>
            @Html.Label("Contraseña")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Contraseña)
        </dd>

        <dt>
            @Html.Label("Tipo de Usuario")
        </dt>

        <dd>
            @Html.DisplayFor(model => model.Tipo_Usuario.Tipo_Usuario1)
        </dd>

    </dl>

    @using (Html.BeginForm()) {
        @Html.AntiForgeryToken()

        <div class="form-actions no-color">
            <input type="submit" value="Eliminar Usuario" class="btn btn-default" /> |
            @Html.ActionLink("Volver", "Index")
        </div>
    }
</div>
